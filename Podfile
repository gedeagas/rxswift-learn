# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'swift-fetch-data' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  

  # Pods for swift-fetch-data
  pod 'RxSwift', '~> 4.0'  
  pod 'RxCocoa', '~> 4.0'
  pod 'Moya/RxSwift'
  pod "Unbox"
  
  target 'swift-fetch-dataTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxSwift',    '~> 4.0'
    pod 'RxCocoa',    '~> 4.0'
    pod 'Moya/RxSwift'
  end

  target 'swift-fetch-dataUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
